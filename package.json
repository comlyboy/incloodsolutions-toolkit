		"libphonenumber-js": "^1.11.16",
		"pdfkit": "^0.15.1",
		"qr-code-styling": "^1.8.4",
		"qrcode": "^1.5.4",
		"uuid": "^11.0.3",
		"validator": "^13.12.0"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.2.2",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.2",
